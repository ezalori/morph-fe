{
  "name": "vault-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode=development --progress",
    "start:local": "cross-env MOCK=none webpack serve --mode=development --progress",
    "build": "cross-env NODE_ENV=production webpack --mode=production --progress",
    "build:ci": "cross-env NODE_ENV=production webpack --mode=production",
    "lint": "eslint --ext .vue,.js --fix . && prettier --write .",
    "lint:ci": "eslint --ext .vue,.js . && prettier --check .",
    "analyze": "cross-env NODE_ENV=production ANALYZE=1 webpack --mode=production --progress",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown",
    "*.{vue,js}": "eslint --fix"
  },
  "dependencies": {
    "@antv/g6": "^2.1.4-beta.3",
    "@vue/composition-api": "^1.4.9",
    "ace-builds": "^1.4.14",
    "axios": "^0.21.2",
    "basscss": "^8.0.2",
    "chart.js": "^2.5.0",
    "element-ui": "^2.15.8",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "normalize.css": "^5.0.0",
    "pinia": "^2.0.14",
    "qs": "^6.9.0",
    "vue": "^2.7.0",
    "vue-cookie": "^1.1.4",
    "vue-router": "^3.5.4",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "babel-loader": "^8.2.4",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "glob": "^7.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.4.2",
    "mini-css-extract-plugin": "^2.6.0",
    "mocker-api": "^2.9.5",
    "prettier": "^2.7.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "vue-loader": "^15.10.0",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "engines": {
    "node": ">=12.22.0"
  }
}
